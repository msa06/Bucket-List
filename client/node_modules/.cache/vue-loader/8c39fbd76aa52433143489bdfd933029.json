 class=\"column input\" v-if=\"isSelected(item)\" v-model=\"editedDescription\" />\n        <p v-else class=\"column\">\n          <span class=\"tag is-primary\">{{ i + 1}}</span>\n          {{ item.description }}\n        </p>\n        <div class=\"column is-narrow\">\n          <span\n            class=\"icon has-text-primary\"\n            @click=\"isSelected(item) ?  unselect() : select(item)\"\n          >\n            <i class=\"material-icons\">{{isSelected(item) ? 'close': 'edit'}}</i>\n          </span>\n\n          <span\n            class=\"icon has-text-info\"\n            @click=\"isSelected(item) ? updateItem(item, i) : removeItem(item, i)\"\n          >\n            <i class=\"material-icons\">{{isSelected(item) ? 'save': 'delete'}}</i>\n          </span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      items: [],\n      description: \"\",\n      editedDescription: \"\",\n      selected: {}\n    };\n  },\n  async mounted() {\n    const response = await axios.get(\"api/bucketListItems/\");\n    this.items = response.data;\n  },\n  methods: {\n    async addItem() {\n      const response = await axios.post(\"api/bucketListItems/\", {\n        description: this.description\n      });\n      this.items.push(response.data);\n      this.description = \"\";\n    },\n    async removeItem(item, i) {\n      await axios.delete(\"api/bucketListItems/\" + item._id);\n      this.items.splice(i, 1);\n    },\n    select(item) {\n      this.selected = item;\n      this.editedDescription = item.description;\n    },\n    isSelected(item) {\n      return item._id === this.selected._id;\n    },\n    unselect() {\n      this.selected = {};\n      this.editedDescription = \"\";\n    },\n    async updateItem(item, i) {\n      const response = await axios.put(\"api/bucketListItems/\" + item._id, {\n        description: this.editedDescription\n      });\n      this.items[i] = response.data;\n      this.unselect();\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  margin: auto;\n  margin-top: 3rem;\n  max-width: 700px;\n}\n.icon {\n  cursor: pointer;\n}\n</style>\n"]}]}