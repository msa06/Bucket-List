A,EAAA,CAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA,KAXA;AAYA,IAAA,MAZA,kBAYA,IAZA,EAYA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,WAAA,iBAAA,GAAA,IAAA,CAAA,WAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,IAhBA,EAgBA;AACA,aAAA,IAAA,CAAA,GAAA,KAAA,KAAA,QAAA,CAAA,GAAA;AACA,KAlBA;AAmBA,IAAA,QAnBA,sBAmBA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,iBAAA,GAAA,EAAA;AACA,KAtBA;AAuBA,IAAA,UAvBA,sBAuBA,IAvBA,EAuBA,CAvBA,EAuBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,yBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,WAAA,EAAA,MAAA,CAAA;AADA,iBAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAIA,gBAAA,MAAA,CAAA,KAAA,CAAA,CAAA,IAAA,QAAA,CAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAMA;AA7BA;AAdA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n    <h1 class=\"subtitle has-text-centered\">Bucket List:</h1>\n    <hr />\n    <div class=\"field has-addons\">\n      <div class=\"control is-expanded\">\n        <input class=\"input\" v-model=\"description\" type=\"text\" placeholder=\"Go to mars...\" />\n      </div>\n      <div class=\"control\">\n        <a class=\"button is-info\" @click=\"addItem\" :disabled=\"!description\">Add</a>\n      </div>\n    </div>\n    <div class=\"notification\" v-for=\"(item, i) in items\" :key=\"item._id\">\n      <div class=\"columns\">\n        <input class=\"column input\" v-if=\"isSelected(item)\" v-model=\"editedDescription\" />\n        <p v-else class=\"column\">\n          <span class=\"tag is-primary\">{{ i + 1}}</span>\n          {{ item.description }}\n        </p>\n        <div class=\"column is-narrow\">\n          <span\n            class=\"icon has-text-primary\"\n            @click=\"isSelected(item) ?  unselect() : select(item)\"\n          >\n            <i class=\"material-icons\">{{isSelected(item) ? 'close': 'edit'}}</i>\n          </span>\n\n          <span\n            class=\"icon has-text-info\"\n            @click=\"isSelected(item) ? updateItem(item, i) : removeItem(item, i)\"\n          >\n            <i class=\"material-icons\">{{isSelected(item) ? 'save': 'delete'}}</i>\n          </span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      items: [],\n      description: \"\",\n      editedDescription: \"\",\n      selected: {}\n    };\n  },\n  async mounted() {\n    const response = await axios.get(\"api/bucketListItems/\");\n    this.items = response.data;\n  },\n  methods: {\n    async addItem() {\n      const response = await axios.post(\"api/bucketListItems/\", {\n        description: this.description\n      });\n      this.items.push(response.data);\n      this.description = \"\";\n    },\n    async removeItem(item, i) {\n      await axios.delete(\"api/bucketListItems/\" + item._id);\n      this.items.splice(i, 1);\n    },\n    select(item) {\n      this.selected = item;\n      this.editedDescription = item.description;\n    },\n    isSelected(item) {\n      return item._id === this.selected._id;\n    },\n    unselect() {\n      this.selected = {};\n      this.editedDescription = \"\";\n    },\n    async updateItem(item, i) {\n      const response = await axios.put(\"api/bucketListItems/\" + item._id, {\n        description: this.editedDescription\n      });\n      this.items[i] = response.data;\n      this.unselect();\n    }\n  }\n};\n</script>\n\n<style>\n#app {\n  margin: auto;\n  margin-top: 3rem;\n  max-width: 700px;\n}\n.icon {\n  cursor: pointer;\n}\n</style>\n"],"sourceRoot":"src"}]}